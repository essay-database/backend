{
  "extends": ["eslint:recommended", "airbnb", "plugin:prettier/recommended"],
  "rules": {
    "no-console": 0,
    "no-undefined": 0,
    "no-use-before-define": 0,
    "no-shadow": 0,
    "global-require": 0,
    "import/no-dynamic-require": 0,
    "no-restricted-syntax": ["off", "ForOfStatement"],
    "no-await-in-loop": 0,
    "no-param-reassign": 0,
    "import/no-extraneous-dependencies": 0
  }
}